<div class="container m-5 p-5">
<br>
<br>
<br>
    <section class="gallery-block compact-gallery">
        <div class="container">
            <div class="heading">
                <h3>Calendario de clases</h3>
                <hr>
            </div>
<p id="notice"><%= notice %></p>

<h1>Agenda una clase</h1>
<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Start</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.start %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><br><br>

<%= link_to 'Nueva clase', new_event_path %>
</div>
